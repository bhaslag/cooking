{
  "name": "bhaslag/cooking",
  "description": "A little project inspired by WildCode School project",
  "type": "project",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8",
      "options": {
        "ssl": {
          "verify_peer": true,
          "allow_self_signed": true
        }
      }
    },
    "smile_drupal": {
      "type": "composer",
      "url": "https://packagist.vitry.intranet/drupal/",
      "options": {
        "ssl": {
          "verify_peer": true,
          "allow_self_signed": true
        }
      }
    },
    "smile_libraries": {
      "type": "composer",
      "url": "https://packagist.vitry.intranet/libraries/",
      "options": {
        "ssl": {
          "verify_peer": true,
          "allow_self_signed": true
        }
      }
    },
    "d3": {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/d3/d3",
          "type": "git",
          "reference": "v3.5.17"
        }
      }
    },
    "highlightjs": {
      "type": "package",
      "package": {
        "name": "components/highlightjs",
        "version": "9.12.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/components/highlightjs",
          "type": "git",
          "reference": "9.12.0"
        }
      }
    }
  },
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "drupal/admin_toolbar": "^1.19.0",
    "drupal/block_visibility_groups": "^1.1.0",
    "drupal/core": "~8.4.2",
    "drupal/drush_language": "1.*",
    "drupal/eu_cookie_compliance": "~1.0.0-beta7",
    "drupal/features": "^3.5.0",
    "drupal/honeypot": "^1.26.0",
    "drupal/metatag": "^1.0",
    "drupal/pathauto": "^1.0",
    "drupal/redis": "1.0.0-beta1",
    "drupal/token": "^1.0",
    "drush/drush": "8.1.15",
    "smile/php-codesniffer-rules": "^4.0",
    "smile/spbuilder": "^4.0",
    "wikimedia/composer-merge-plugin": "~1.3"
  },
  "require-dev": {
    "behat/mink": "1.7.x-dev",
    "behat/mink-goutte-driver": "~1.2",
    "components/highlightjs": "9.12.*",
    "d3/d3": "3.5.*",
    "drupal/coder": "8.2.*",
    "drupal/config_inspector": "1.*",
    "drupal/console": "1.*",
    "drupal/devel": "1.*",
    "drupal/devel_a11y": "1.*",
    "drupal/drupal-extension": "3.*",
    "drupal/examples": "1.*",
    "drupal/pistachio": "1.*",
    "drupal/renderviz": "1.*",
    "drupal/search_kint": "1.*",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "_readme": [
      "By default Drupal loads the autoloader from ./vendor/autoload.php.",
      "To change the autoloader you can edit ./autoload.php."
    ],
    "enable-patching": true,
    "patches": {
    },
    "installer-paths": {
      "src/core": ["type:drupal-core"],
      "src/modules/contrib/{$name}": ["type:drupal-module"],
      "src/modules/custom/{$name}": ["type:drupal-custom-module"],
      "src/profiles/contrib/{$name}": ["type:drupal-profile"],
      "src/profiles/custom/{$name}": ["type:drupal-custom-profile"],
      "src/themes/contrib/{$name}": ["type:drupal-theme"],
      "src/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "src/libraries/{$name}": ["type:drupal-library"],
      "drush/commands/{$name}": ["type:drupal-drush"],
      "integration": ["type:integration"],
      "scripts/ci": ["type:project-ci"]
    },
    "drupal-scaffold": {
      "excludes": [
        "web.config",
        "sites/development.services.yml",
        "sites/example.settings.local.php"
      ]
    }
  },
  "config": {
    "bin-dir": "./bin",
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {

    }
  },
  "autoload": {
    "psr-4": {
      "Drupal\\Core\\Composer\\": "src/core/lib/Drupal/Core/Composer"
    },
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "generate-assets": "DrupalProject\\composer\\ScriptHandler::compileAssets",
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
    "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
    "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
    "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
    "post-install-cmd": "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
    "post-update-cmd": "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
  }
}
